{
    "manifest_version": 3,
    "name": "BudgetManagement",
    "version": "1.0",
    "description": "This extension is for tracking and limiting the budget!",
    "icons": {
        "128": "money128.png",
        "48": "money48.png",
        "16": "money16.png"
    },
   "action": {
    "default_icon": "money16.png",
    "default_popup": "popup.html"
   },
   "options_page": "options.html",
   "background": {
        "service_worker": "eventPage.js"
   },
   "permissions":[
    "storage",
    "notifications",
    "contextMenus"
   ]
}